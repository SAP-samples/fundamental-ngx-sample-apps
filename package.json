{
  "name": "ngx-sample-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "npm run copy-app-docs-theming && ng",
    "publish": "npm run copy-app-docs-theming && ng build --prod && ng add angular-cli-ghpages && ng deploy",
    "start": "npm run copy-app-docs-theming && ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-app-docs-theming": "npm run copy-light-theme && npm run copy-dark-theme && npm run copy-hcb-theme && npm run copy-hcw-theme && npm run copy-belize-theme",
    "copy-belize-theme": "cp node_modules/@sap-theming/theming-base-content/content/Base/baseLib/sap_belize/css_variables.css src/assets/sap_belize.css",
    "copy-light-theme": "cp node_modules/@sap-theming/theming-base-content/content/Base/baseLib/sap_fiori_3/css_variables.css src/assets/sap_fiori_3.css",
    "copy-dark-theme": "cp node_modules/@sap-theming/theming-base-content/content/Base/baseLib/sap_fiori_3_dark/css_variables.css src/assets/sap_fiori_3_dark.css",
    "copy-hcb-theme": "cp node_modules/@sap-theming/theming-base-content/content/Base/baseLib/sap_fiori_3_hcb/css_variables.css src/assets/sap_fiori_3_hcb.css",
    "copy-hcw-theme": "cp node_modules/@sap-theming/theming-base-content/content/Base/baseLib/sap_fiori_3_hcw/css_variables.css src/assets/sap_fiori_3_hcw.css",
    "deploy": "ng build --prod && npx ngh --dir dist/ngx-sample-app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.0",
    "@angular/cdk": "^11.0.1",
    "@angular/common": "11.0.0",
    "@angular/compiler": "11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/fire": "^6.1.3",
    "@angular/forms": "^11.0.0",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/router": "^11.0.0",
    "@fundamental-ngx/core": "v0.24.2",
    "@types/jest": "^26.0.16",
    "angular-draggable-droppable": "^4.5.6",
    "angularfire2": "^5.4.2",
    "core-js": "^3.7.0",
    "hammerjs": "^2.0.8",
    "ngx-cookie-service": "^11.0.2",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "^11.0.0",
    "@angular/language-service": "^11.0.2",
    "@sap-theming/theming-base-content": "^11.1.23",
    "@types/node": "^14.14.10",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.1",
    "firebase": "^8.1.1",
    "ts-node": "^9.0.0",
    "tslint": "~6.1.3",
    "typescript": "4.0.5"
  }
}
